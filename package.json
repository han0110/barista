{
  "name": "object-scanner",
  "version": "1.0.0",
  "description": "object scanner visualizer",
  "main": "index.js",
  "repository": "https://github.com/han0110/object-scanner",
  "author": "Jason Jian",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --progress",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "start": "node lib/server.js",
    "lint:js": "eslint --cache --format=node_modules/eslint-formatter-pretty . && flow .",
    "lint:css": "stylelint src/*.scss src/components/*.scss --syntax scss",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "clean": "rimraf dist && rimraf flow-typed",
    "deploy": "yarn clean && yarn build && gh-pages -d dist -b gh-pages -m '☄️ Deploy'"
  },
  "dependencies": {
    "koa": "^2.5.0",
    "koa-static": "^4.0.2",
    "pixi.js": "^4.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "request-promise": "^4.2.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.59.0",
    "flow-typed": "^2.2.3",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
